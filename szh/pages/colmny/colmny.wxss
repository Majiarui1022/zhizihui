.wrap{
  width: 100%;
  height:288rpx;
  position: relative
}
.slider{
  width: 100%;
  height:288rpx;
  background: #666;
}
.slider image{
  width: 100%;
  height:100%;
  display: block;
}

/* 款项 */
.money{
  width: 100%;
  height:160rpx;
  position: relative;
}
.money-menu{
  width: 690rpx;
  position: absolute;
  left: 50%;
  margin-left: -345rpx;
  top:-30rpx; 
  display: flex;
  box-sizing: border-box;
  padding:30rpx 0;
  background: #fff;
  box-shadow:0px 3px 7px 0px rgba(0, 0, 0, 0.41);
  border-radius:10px;
}
.money-left,.money-cont,.money-right{
  width: 33.33%;
  height:100rpx;
  text-align: center;
  border-right: 1rpx solid #ECECEC;
}
.money .money-right{
  border-right:0;
}
.mu-price,.yi-price,.pro-num{
  font-size:51rpx;
  font-weight:bold;
  color:rgba(67,172,67,1);
  display: block;
  text-align: center;
}
.money-right .pro-num{
  font-size: 48rpx;
}
.size-c{
  font-size:24rpx;
  color:rgba(153,153,153,1);
  display: block;
  text-align: center;
}


/* 主体 */
.menu-con{
  width: 100%;
  box-sizing: border-box;
  padding:0 30rpx;
}
.qi-logo{
  width: 100%;
  height:auto;
  border-bottom: 1rpx solid #EAEAEA;
}
.logo-img{
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.logo-img image{
  width: 78rpx;
  height: 78rpx;
  padding-right: 30rpx;
  margin-right: 10rpx;
  margin-bottom:55rpx;
  border-right: 1rpx solid #ECECEC;
  margin-left: 19rpx;
}
.logo-img image:nth-child(5n){
  border:0;
}
.logo-img image:last-child{
  border:0;
}

/* 筹款内容 */
.cont-word{
  height:240rpx;
}
.chou-tit{
  font-size:38rpx;
  font-weight:bold;
  color:#333333;
  line-height:78rpx;
  display: block;
}
.chou-wen{
  /* width:671px;
  height:157px; */
  font-size:32rpx;
  color:rgba(102,102,102,1);
  padding-left: -60px;
  text-indent: 60rpx;
}
.show-wv{
  width: 100%;
  position: relative;
  padding-bottom: 140rpx;
}
.show-word{
  width: 100%;
  display: flex;
  align-items: center;
}
/* 遮罩层 */
.zhe{
width:100%;
height:51px;
background:linear-gradient(0deg,rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
position:absolute;
left: 0;
top:-80rpx;
}
.word-tit{
  width:160rpx;
  height:39rpx;
  background:rgba(205,253,205,1);
  border-radius:20px;
  margin:0 auto;
  font-size:20rpx;
  color:rgba(7,199,21,1);
  line-height: 39rpx;
  text-indent: 27rpx;
  z-index: 100000;
}


/* 视频 */
.show-viid{
  width: 100%;
  height:323rpx;
  margin-top:32rpx;
  background: #C1C1C1;
}



/* 底部 */
.footer{
  width: 100%;
  height:100rpx;
  position: fixed;
  background: #FFF;
  left: 0;
  bottom: 0;
  box-sizing: border-box;
  padding-left: 32rpx;
  z-index: 9999;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-shadow:0px 6rpx 12rpx 0px rgba(0, 0, 0, 0.14);
}
.footer-zhuan{
  width: 100rpx;
  height:100%;
  display: flex;
  flex-direction: column;

}
.zhuan{
  display: block;
  width: 37rpx;
  margin:14rpx auto;
  height:35rpx;
}
.fa-word{
  font-size: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.footer-mny{
  flex: 1;
  height:100%;
  display: flex;
  align-items: center
}
.footer-mny>view{
  font-size:24rpx;
  margin-right: 20rpx;
}
.footer-mny>view>image{
  width: 20rpx;
  height: 20rpx;
}
.zhifu{
  width: 235rpx;
  height:100%;
  text-align: center;
  line-height: 100rpx;
  background: #43AC43;
}
.radio-group{
  display: flex;
}

.footer-money{
  font-size: 32rpx;
}
.checkbosx{
  position: relative;
}
